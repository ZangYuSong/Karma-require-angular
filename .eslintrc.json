{
  "env": {
    "browser": true,
    "node": true,
    "jasmine": true
  },
  "extends": "eslint:recommended",
  "rules": {
    "indent": ["error", 2],
    "quotes": ["error", "double"],
    "semi": ["error", "always"],
    "key-spacing": [
      0,
      {
        "beforeColon": false,
        "afterColon": true
      }
    ],
    "quote-props": ["error", "always"],
    "guard-for-in": 0,
    "operator-linebreak": ["error", "after"],
    "eqeqeq": ["error", "always"],
    "guard-for-in": "error",
    "no-eval": "error",
    "no-else-return": "error",
    "radix": "error",
    "camelcase": "error",
    "no-with": "error"
  }
}
